#update account jest mozliwe tez poprzez wyslanie patch na account/{accountId}
{
    "email" : "newemail@gmail.com",
    "password": " newpassword"
}

#dodanie nowej roli mozliwe jest poprzez tylko poprzez   PATCH na /users/{userId}
{
	"roleName":"TESTER",
	"userId" : "1"
}




